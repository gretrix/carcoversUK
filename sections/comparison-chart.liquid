    .chart-container {
      display: flex;
      flex-direction: column;
      align-items: center;
      margin: auto;
      overflow-x: hidden;
      max-width: 1400px;
      margin-bottom: 30px;
      font-family: var(--font-body-family);
    }

    .ourcovershead {
      text-align: center;
      font-weight: bold;
      font-size: 35px;
      margin: 10px 0px;
      padding: 0px;
      font-family: var(--font-heading-family);
    }

    .ourcoverstext {
      text-align: center;
      width: 55%;
      line-height: 18px;
    }
    .comparisoncharthead {
      
      vertical-align: top;
    }

    .comparisoncharthead > img {
      width: 100%;
    }
    .comparisoncharttable {
      border-collapse: collapse;
      border-spacing: 0;
      /*             width: 98%; */
      border: none !important;
      display: block;
    }

    /* Style table headers and table data */
    .comparisoncharthead,
    .comparisonchartdata {
      text-align: center;
      padding: 12px;
    }
    .comparisoncharthead {
      font-size: 22px;
    }
    .comparisoncharthead:first-child,
    .comparisonchartdata:first-child {
      text-align: right;
    }

    .comparisonchartdata:first-child {
      font-weight: bold;
      font-style: italic;
    }

    /* Zebra-striped table rows */
    .comparisonchartrow:nth-child(odd):not(:nth-of-type(0)) {
      background-color: lightgray;
    }
   
    .clip-star {
      background: #ff9933;
      clip-path: polygon(
        50% 0%,
        61% 35%,
        98% 35%,
        68% 57%,
        79% 91%,
        50% 70%,
        21% 91%,
        32% 57%,
        2% 35%,
        39% 35%
      );
      display: inline-block !important;
      height: 1.5em;
      width: 1.5em;
    }

    .clip-star-container {
      text-align: center;
      margin: auto;

      /*             padding-left: 5%;
            padding-right: 5%; */
    }

    .clip-star-container-plus {
      text-align: center;
      margin: auto;

      /*             padding-left: 3%;
            padding-right: 2%; */
    }
    .clip-star-container:last-child {
      /*             padding-left: 4%;
            padding-right: 2%; */
    }

    .plus {
      display: inline-block !important;
      width: 24px;
      height: 24px;

      background: linear-gradient(#fff, #fff), linear-gradient(#fff, #fff),
        #ff9933;
      background-position: center;
      background-size: 50% 2px, 2px 50%; /*thickness = 2px, length = 50% (25px)*/
      background-repeat: no-repeat;
    }

    span {
      content: "\2713";
    }

    .button-new {
      background-color: #336699; /* blue */
      color: white;
      padding: 10px 10px;
      text-align: center;
      font-size: 17px;
      font-weight: bold;
      border-radius: 8px;
      margin: 0px;
      text-decoration: none;
    }
    .comparisonchartrow .button-new {
      display: block;
    }

    .button-new:hover {
      cursor: pointer;
    }

    .button-new:active {
      background-color: #4caf5050;
    }

    .carcoverimg {
      height: auto;
      width: 70%;
    }

    .best {
    
            background-color: #3366ff;
        
          
    }

