= 'Snowmobiles' %} {% assign main_product_website = 'Snowmobilecover.com' %}
<script>
  {% assign urlCheck = canonical_url | split: '/' %}

  {% if urlCheck[5] contains 'car' or urlCheck[5] == null %}
    {% assign chartData = collections["car-covers"].products %}
  {% elsif urlCheck[5] contains 'suv' %}
    {% assign chartData = collections["suv-covers"].products%}
  {% elsif urlCheck[5] contains 'truck' %}
    {% assign chartData = collections["truck-covers"].products%}
  {% elsif urlCheck[5] contains 'van' %}
    {% assign chartData = collections["van-covers"].products%}
  {% elsif urlCheck[5] contains 'motorcycle' %}
    {% assign chartData = collections["motorcycle-covers"].products%}
  {% elsif urlCheck[5] contains 'scooter' %}
    {% assign chartData = collections["scooter-covers"].products %}
    {% elsif urlCheck[5] contains 'jet_ski' %}
     {% paginate collection.products by 4 %}
       {% assign chartData = collections["jet-ski-covers"].products %}
      {{- paginate | default_pagination }}
    {% endpaginate %}
        
      
    {% elsif urlCheck[5] contains 'snowmobile' %}
    {% assign chartData = collections["snowmobile-covers"].products %}
    
  {% endif %}
  console.log({{ chartData | json}}, "<----------------")

</script>

