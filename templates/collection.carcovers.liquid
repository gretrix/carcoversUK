<!-- Page Level CSS -->
<style>
  .customer-photo{
  	width: 30px;
  }
  .overall-rating{
   	display: block;
    text-align: center; 
  }
  .vehicle-title{
   	text-transform:capitalize 
  }
  .prod-sel-body-header a, .prod-sel-nb-header a.nostyle:link {
    text-decoration: inherit;
    color: inherit;
  }

  .prod-sel-body-header a, .prod-sel-nb-header a.nostyle:visited {
      text-decoration: inherit;
      color: inherit;
  }
</style>


<!-- Page Level JS -->
<script>
  	cartData = {{ cart | json }};
  
  	productsObject = {{ collections.[collection.handle].products | json }};
  
  	urlVehicleInfo = window.location.pathname.match(/-covers\/([^-]*)-([^-]*)-([^-]*)-*([^-]*)/);
  	urlVehicleInfoPath = window.location.pathname.match(/-covers\/([^-]*-[^-]*-[^-]*-*[^-]*)/)[1];

    vehicleInfo = {
        "make": urlVehicleInfo[1].replace(/_/g, " "),
        "model": urlVehicleInfo[2],
        "year": urlVehicleInfo[3],
        "bodytype": urlVehicleInfo[4] ? urlVehicleInfo[4].replace(/_/g, " ") : ""
    }
    
    vehicleInfoString = vehicleInfo.year + " " + vehicleInfo.make + " " + vehicleInfo.model + " " + vehicleInfo.bodytype;
    

  
  	{% for product in collections.[collection.handle].products %}
  		covers = "{{ product.tags | join: "," }}";
        covers = covers.split(",");
  		
  		for (let i = 0; i < covers.length; i++) {
          element = document.querySelector("."+ covers[i]);
          if(element != null)
          {
            element.querySelector(".add-to-cart-btn").addEventListener("click", function(event){
                event.preventDefault();

                data = {
                    items: 
                    [
                      {
                        quantity: 1,
                        id: {{ product.variants[0].id }},
                        properties: {
                          "vehicle-info": vehicleInfoString
                        }
                      }
                    ]
                }

                var xhr = new XMLHttpRequest();
                xhr.open("POST", window.Shopify.routes.root + 'cart/add.js', true);
                xhr.setRequestHeader('Content-Type', 'application/json');
                xhr.send(JSON.stringify(data));
            });
            
           	element.style.display = "block";
            element.querySelector(".prod-sel-body-header a, .prod-sel-nb-header a").href = "{{ product.url }}/"+urlVehicleInfoPath;
            element.querySelector(".vehicle-title").innerText = vehicleInfoString;
            element.querySelector(".prod-sel-nb-main-price").innerText = "$"+({{ product.variants[0].price}}/100);
            element.querySelector(".prod-sel-nb-main-reg-price").innerText = "$"+({{ product.variants[0].compare_at_price}}/100);
          }
        }
	{% endfor %}
</script>